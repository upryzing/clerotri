{
  "name": "rvmob",
  "version": "0.8.0",
  "homepage": "https://upryzing.github.io/clerotri",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "clean": "react-native clean",
    "android": "yarn prebuild && rnx-cli run-android --active-arch-only --appIdSuffix=debug --mode=debug",
    "android-release": "yarn prebuild && yarn android-release:bundle && rnx-cli build-android --tasks assembleRelease",
    "android-release:bundle": "rnx-cli bundle --dev false && yarn android-release:copy-assets",
    "android-release:copy-assets": "mkdir -p android/app/build/generated/assets/createBundleReleaseJsAndAssets && mkdir -p android/app/build/generated/res/createBundleReleaseJsAndAssets && cp dist/android/index.android.bundle.hbc android/app/build/generated/assets/createBundleReleaseJsAndAssets/index.android.bundle && cp -r dist/android/res android/app/build/generated/res/createBundleReleaseJsAndAssets",
    "android-release:full": "yarn android-release && rm android/app/build/outputs/apkset/release/app-release.apks -f && rnx-cli build-android --tasks buildApksRelease",
    "ios": "react-native run-ios",
    "start": "react-native rnx-start",
    "test": "jest",
    "lint": "eslint .",
    "license-list": "yarn licenses list --production --json --recursive > assets/data/licenses.json && node scripts/licenseListProcessor.mjs -silent",
    "metadata": "node scripts/metadataGenerator.mjs",
    "prebuild": "yarn metadata && yarn license-list -prod",
    "web": "yarn prebuild && vite -c config/vite.config.ts",
    "web-release": "yarn prebuild && vite build -c config/vite.config.ts",
    "web-preview": "yarn prebuild && vite preview -c config/vite.config.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.2",
    "@notifee/react-native": "^9.1.8",
    "@react-native-clipboard/clipboard": "1.16.2",
    "@react-native-community/hooks": "^100.1.0",
    "@react-native-documents/picker": "^10.1.1",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/material-design-icons": "^7.4.47",
    "@react-native-vector-icons/material-icons": "^0.0.1",
    "@rexovolt/react-native-markdown-display": "^9.1.1",
    "@traptitech/markdown-it-spoiler": "patch:@traptitech/markdown-it-spoiler@npm%3A1.1.6#~/.yarn/patches/@traptitech-markdown-it-spoiler-npm-1.1.6-ee0dc02626.patch",
    "@xmldom/xmldom": "^0.9.8",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.3",
    "intl-pluralrules": "^2.0.1",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "^0.78.0",
    "react-native-device-info": "^14.0.4",
    "react-native-drawer-layout": "^4.1.1",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-controller": "^1.16.7",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "3.17.1",
    "react-native-reanimated-image-viewer": "^1.0.2",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-svg": "^15.11.2",
    "react-native-web": "^0.19.13",
    "reanimated-color-picker": "^4.0.0",
    "revolt.js": "npm:@rexovolt/revolt.js@^6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/runtime": "^7.26.10",
    "@react-native-community/cli": "^15.1.3",
    "@react-native-community/cli-platform-android": "^15.1.3",
    "@react-native-community/cli-platform-ios": "^15.1.3",
    "@react-native/babel-preset": "^0.78.0",
    "@react-native/eslint-config": "^0.78.0",
    "@react-native/metro-config": "^0.78.0",
    "@react-native/typescript-config": "^0.78.0",
    "@rnx-kit/babel-preset-metro-react-native": "^2.0.0",
    "@rnx-kit/cli": "^0.18.6",
    "@rnx-kit/metro-config": "^2.0.1",
    "@rnx-kit/metro-resolver-symlinks": "^0.2.1",
    "@svgr/rollup": "^8.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/eslint": "^8.56.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-native-web": "^0.19.0",
    "@types/react-test-renderer": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-native-web": "^0.19.13",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.1",
    "eslint-plugin-de-morgan": "^1.2.0",
    "jest": "^29.7.0",
    "knip": "^5.46.0",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-react-native-web": "^2.1.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.6.0",
  "rnx-kit": {
    "bundle": {
      "entryFile": "index.js",
      "targets": [
        "android"
      ],
      "platforms": {
        "android": {
          "bundleOutput": "dist/android/index.android.bundle",
          "sourcemapOutput": "dist/android/index.android.bundle.map",
          "assetsDest": "dist/android/res"
        }
      },
      "treeShake": true,
      "hermes": true,
      "plugins": [
        [
          "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
          {
            "includeNodeModules": false,
            "linesOfContext": 3,
            "throwOnError": true
          }
        ],
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "ignoredModules": [
              "react-is"
            ]
          }
        ]
      ]
    }
  },
  "resolutions": {
    "@rnx-kit/metro-serializer-esbuild@npm:^0.2.1": "patch:@rnx-kit/metro-serializer-esbuild@npm%3A0.2.1#~/.yarn/patches/@rnx-kit-metro-serializer-esbuild-npm-0.2.1-1e72c4b48a.patch"
  }
}
